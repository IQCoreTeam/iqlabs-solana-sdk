{
  "name": "iqlabs-solana-sdk",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IQCoreTeam/iqlabs-solana-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/IQCoreTeam/iqlabs-solana-sdk/issues"
  },
  "homepage": "https://github.com/IQCoreTeam/iqlabs-solana-sdk#readme",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./idl/code_in.json": "./dist/idl/code_in.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc && tsc -p tsconfig.esm.json && cp -R idl dist/idl && cp -R idl dist/esm/idl",
    "example:hello": "npm run build && tsx examples/hello.ts",
    "prepare": "npm run build",
    "test:contract": "tsx tests/contract/smoke.ts",
    "test:sdk": "tsx tests/sdk/smoke.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@noble/hashes": "^1.8.0",
    "@solana/web3.js": "^1.98.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
